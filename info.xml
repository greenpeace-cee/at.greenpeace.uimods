<?xml version="1.0"?>
<extension key="at.greenpeace.uimods" type="module">
  <file>uimods</file>
  <name>UI Modifications</name>
  <description>UI Modifications for Greenpeace.at</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/at.greenpeace.uimods</url>
    <url desc="Documentation">https://github.com/systopia/at.greenpeace.uimods</url>
    <url desc="Support">https://github.com/systopia/at.greenpeace.uimods/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-11-11</releaseDate>
  <version>1.10.2-beta.1</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Uimods</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
    <mixin>scan-classes@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_Uimods_Upgrader</upgrader>
</extension>
